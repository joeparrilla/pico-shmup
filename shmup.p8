function _init()
	game_over = false
	make_player()
	make_enemies()
end

function _update()
	if(not game_over) then
		update_enemies()
		check_enemy_collision()
		move_player()
		check_hit()
	else
 	if (btnp(5)) _init() --restart
	end
end

function _draw()
	cls()
	draw_player()
	draw_enemies()
	
	if (game_over) then
 	print("game over!",44,44,7)
	 print("your score:"..player.score,34,54,7)
  print("press âŽ to play again!",18,72,6)
 else
 	print("score:"..player.score,2,2,7)
 end
end
